        <div class="coin">
            <img id="coin-label" src="/src/resource/img/icon/라벨2.png">
        </div>
        <!-- 보유캐릭터 상점 -->
        <div class="closet-btn">
            <img id="my-skin" src="/src/resource/img/icon/기본버튼.png">
