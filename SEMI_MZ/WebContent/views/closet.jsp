<body>
    <div class="myroom">
        <!-- 유저 -->
        <div class="user">
            <img id="user-skin" src="/src/resource/img/user/skin01/fs.png" alt="유저캐릭터">
        </div>
        <!-- 옷장 -->
        <div class="closet">
            <img id="furniture" src="/src/resource/img/icon/빈옷장.png">
